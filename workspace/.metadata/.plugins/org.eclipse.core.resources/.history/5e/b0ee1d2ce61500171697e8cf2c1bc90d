import java.io.IOException;
import java.util.List;

public class NetSpider {
	/**
	* @param args
	 * @throws IOException 
	*/
	public static void main(String[] args) throws IOException {
//		1、将文件封装成对象
		File file=new File("mail.html");
		List<String> mailList=getMails(file);
