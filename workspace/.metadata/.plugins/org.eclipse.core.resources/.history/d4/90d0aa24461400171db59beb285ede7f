package cn.inprint.demo;

import java.io.PrintStream;

public class printDemo {
	/**
	* @param args
	* IO包中的其他功能流对象
	* 功能流对象
	* 特点：解决的问题,特有的方法
	* 打印流--输出流
	* PrintStream(字节流)  PrintWriterStrean(字符流)
	* 特点：打印
	* 解决问题：
	*/
	public static void main(String[] args) {
		PrintStream pStream=System.out;
		pStream.println("HI");
//		等同于
		System.out.println("HI");
	}
}
