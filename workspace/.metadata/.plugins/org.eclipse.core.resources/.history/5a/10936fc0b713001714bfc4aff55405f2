package cn.mybuffer.demo;

public class TestDemo {
	/**
	* @param args
	*/	
	public static void main(String[] args) {
	/*	
//		需求：定义一个功能，记录程序运行的次数。满足5次后，给出提示，试用次数已到，请注册！
		思路：
		1、需要计数器
	*/	
		boolean b=checkOut();
	}

	/**
	 * @return
	 */
	public static boolean checkOut() {
		int count=0;
		count++;
		System.out.println("count="+count);
		return count;
	}
}
