package com.example.helloworld;

public class UsrInput {
	public static void main(String[] args){
//		循环结构
		int x=-1;
		while(x<3){
			System.out.println("x="+x);
			x++;
		} 
//		需求：求1-10的和
		int a=1,sum=0;
		while(a<=10){
			sum+=a;
			a++;
		}
		System.out.println("sum="+sum);
	}
}
